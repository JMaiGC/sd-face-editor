{
    "face_detector": {
        "name": "YOLO",
        "params": {
            "path": "yolov8n.pt"
        }
    },
    "rules": [
        {
            "when": {
                "tag": "person"
            },
            "then": {
                "face_processor": "img2img",
                "mask_generator": "BiSeNet"
            }
        },
        {
            "then": {
                "face_processor": "img2img",
                "mask_generator": "Rect"
            }
        }
    ]
}